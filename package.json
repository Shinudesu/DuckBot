{
  "name": "duck-bot",
  "version": "1.0.0",
  "description": "Discord bot for private server",
  "main": "dist/index.js",
  "repository": "https://github.com/Shinudesu/DuckBot.git",
  "author": "shinudesu",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=14.x",
    "yarn": ">=1.x"
  },
  "scripts": {
    "build": "tsc",
    "clean": "npx rimraf dist",
    "lint": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "start": "cross-env NODE_ENV=production node -r ./tsconfig-paths-bootstrap.js dist/src/index.js",
    "start:dev": "cross-env NODE_ENV=development ts-node --files -r tsconfig-paths/register src/index.ts",
    "quickstart": "yarn clean && yarn build && yarn start",
    "test": "jest",
    "tsc": "tsc"
  },
  "dependencies": {
    "@heroku-cli/color": "^1.1.14",
    "axios": "^0.21.1",
    "common-tags": "^1.8.0",
    "cross-env": "^7.0.3",
    "detectlanguage": "^2.1.0",
    "discord-akairo": "https://github.com/discord-akairo/discord-akairo.git#master",
    "discord.js": "^13.0.0-dev.fe6cc0c.1625227356",
    "dotenv": "^10.0.0",
    "emoji-regex": "^9.2.2",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "node-cron": "^3.0.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "random-animals-api": "^1.2.6",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.1.0",
    "snoowrap": "^1.23.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.4",
    "tz-offset": "^0.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/node": "^15.12.2",
    "@types/node-cron": "^2.0.3",
    "@types/validator": "^13.1.4",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-ruppy-jest": "^1.0.0",
    "eslint-config-ruppy-node": "^1.5.2",
    "eslint-config-ruppy-ts": "^1.6.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.1",
    "tslint": "^6.1.3"
  }
}
